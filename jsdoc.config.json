{
  "source": {
  "include": ["./src/"],
    "includePattern": ".+\\.js(doc)?$"
},
  "plugins": [
  "plugins/markdown"
],
  "opts": {
  "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true,
    "tutorials": "./info/"
},
  "templates": {
  "default": {
    "outputSourceFiles": false
  }
}
}
